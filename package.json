{
  "name": "wishlist-api-server",
  "version": "0.0.1",
  "description": "wishlist api server",
  "scripts": {
    "test": "jest --forceExit --coverage --verbose",
    "tsc": "tsc",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --fix-dry-run",
    "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
